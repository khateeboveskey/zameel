{
  "expo": {
    "name": "زميل",
    "description": "زميل هو نظام إدارة محتوى الطالب",
    "slug": "zameel",
    "scheme": "zameel",
    "owner": "Bitwise",
    "githubUrl": "https://github.com/khateeboveskey/zameel",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "primaryColor": "#6366f1",
    "splash": {
      "backgroundColor": "#ffffff",
      "image": "./src/assets/splash.png",
      "resizeMode": "contain"
    },
    "icon": "./src/assets/icon.png",
    "plugins": [
      "expo-font",
      "expo-router",
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ]
    ],
    "extra": {
      "supportsRTL": true,
      "forcesRTL": true,
      "eas": {
        "projectId": "38d9edf1-d9ce-45d9-b5cd-624f4848ee80"
      }
    },
    "android": {
      "package": "com.bitwise.zameel"
    }
  }
}
